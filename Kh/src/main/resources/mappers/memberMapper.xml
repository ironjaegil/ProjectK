<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.project.mapper.memberMapper">
	<select id="selectMember" resultType="memberVO">
		SELECT  rownum 	as ID, 
                ID 		as USER_ID, 
        		PWD,
        		NAME, 
        		INDATE 
          FROM MEMBER
    </select>
</mapper>


